    $(function(){
        var options = {
            series: [{
                data: [3, 16, 2, 14, 15, 7, 9, 21, 7, 5, 4],
            },{
                data: [11, 2, 3, 1, 10, 13, 7, 8, 21, 22, 3],
            }],
            chart: {
                type: 'line',
